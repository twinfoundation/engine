{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"description": "Auto-Generated Release Please configuration for patch",
	"pull-request-title-pattern": "chore: patch release prepared",
	"pull-request-header": "chore: patch release prepared",
	"pull-request-footer": "This PR was generated by the prepare-release GHA",
	"release-type": "node",
	"versioning": "always-bump-patch",
	"packages": {
		"packages/engine-models": {
			"package-name": "engine-models",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/engine-core": {
			"package-name": "engine-core",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/engine-types": {
			"package-name": "engine-types",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/engine-server-types": {
			"package-name": "engine-server-types",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/engine": {
			"package-name": "engine",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/engine-server": {
			"package-name": "engine-server",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		}
	},
	"plugins": [
		"node-workspace",
		{
			"type": "linked-versions",
			"groupName": "repo",
			"components": [
				"engine-models",
				"engine-core",
				"engine-types",
				"engine-server-types",
				"engine",
				"engine-server"
			]
		}
	]
}
